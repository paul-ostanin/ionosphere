set terminal png size 1600, 1200
set output "stationary_10_nz_phi_360.png"
set format x "%g"
set title "Convergence in z (360 nodes in phi), phi = 10"
set xlabel "n, cm^{-3}"
set ylabel "z, km"
set logscale x
plot "step2_10_nz_80_nphi_360_tau_5.txt" using 2:1 wi li lw 2 title "Nz = 80", "step2_10_nz_200_nphi_360_tau_5.txt" using 2:1 wi li lw 2 title "Nz = 200", "step2_10_nz_400_nphi_360_tau_5.txt" using 2:1 wi li lw 2 title "Nz = 400"
