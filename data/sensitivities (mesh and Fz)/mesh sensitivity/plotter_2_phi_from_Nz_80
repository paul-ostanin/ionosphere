set terminal png size 1600, 1200
set output "stationary_2_phi_nz_80.png"
set format x "%g"
set title "Convergence in phi (80 nodes in z), phi = 2"
set xlabel "n, cm^{-3}"
set ylabel "z, km"
set logscale x
plot "step2_02_nz_80_nphi_180_tau_5.txt" using 2:1 wi li lw 2 title "Nphi = 180", "step2_02_nz_80_nphi_360_tau_5.txt" using 2:1 wi li lw 2 title "Nphi = 360", "step2_02_nz_80_nphi_720_tau_5.txt" using 2:1 wi li lw 2 title "Nphi = 720"
