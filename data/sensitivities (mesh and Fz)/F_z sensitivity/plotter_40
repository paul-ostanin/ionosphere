set terminal png size 1640, 1240
set output "sensitivity_f_z_phi_40.png"
set format x "%g"
set title "F_z sensitivity, phi = 40"
set xlabel "n, cm^{-3}"
set ylabel "z, km"
set logscale x
plot "step2_40_fz_1e9.txt" using 2:1 wi li lw 2 title "F_z = 10^9", "step2_40_fz_5e8.txt" using 2:1 wi li lw 2 title "F_z = 5*10^8", "step2_40_fz_25e7.txt" using 2:1 wi li lw 2 title "F_z = 25*10^7", "step2_40_fz_0.txt" using 2:1 wi li lw 2 title "F_z = 0", "step2_40_fz_-25e7.txt" using 2:1 wi li lw 2 title "F_z = -25*10^7", "step2_40_fz_-5e8.txt" using 2:1 wi li lw 2 title "F_z = -5*10^8", "step2_40_fz_-1e9.txt" using 2:1 wi li lw 2 title "F_z = -10^9"
