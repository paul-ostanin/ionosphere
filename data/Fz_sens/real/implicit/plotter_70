set terminal png size 1670, 1270
set output "sensitivity_impl_f_z_phi_70.png"
set format x "%g"
set title "F_z sensitivity, implicit model, phi = 70"
set xlabel "n, cm^{-3}"
set ylabel "z, km"
set logscale x
plot "step2_impl_70_fz_+1e9.txt" using 2:1 wi li lw 2 title "F_z = 10^9", "step2_impl_70_fz_+5e8.txt" using 2:1 wi li lw 2 title "F_z = 5*10^8", "step2_impl_70_fz_25e7.txt" using 2:1 wi li lw 2 title "F_z = 25*10^7", "step2_impl_70_fz_0.txt" using 2:1 wi li lw 2 title "F_z = 0", "step2_impl_70_fz_-25e7.txt" using 2:1 wi li lw 2 title "F_z = -25*10^7", "step2_impl_70_fz_-5e8.txt" using 2:1 wi li lw 2 title "F_z = -5*10^8", "step2_impl_70_fz_-1e9.txt" using 2:1 wi li lw 2 title "F_z = -10^9"
