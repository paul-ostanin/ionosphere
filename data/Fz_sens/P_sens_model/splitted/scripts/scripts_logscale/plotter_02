set terminal png size 1600, 1200
set output "sensitivity_model_split_f_z_phi_02.png"
set format x "%g"
set title "F_z sensitivity, model solution, splitted scheme, phi = 2"
set xlabel "n, cm^{-3}"
set ylabel "z, km"
set logscale x
plot "step2_02_fz_0.txt" using 2:1 wi li lw 2 title "F_z * 1.00", "step2_02_fz_+05.txt" using 2:1 wi li lw 2 title "F_z * 1.05", "step2_02_fz_+10.txt" using 2:1 wi li lw 2 title "F_z * 1.10", "step2_02_fz_+15.txt" using 2:1 wi li lw 2 title "F_z * 1.15", "step2_02_fz_-05.txt" using 2:1 wi li lw 2 title "F_z * 0.95", "step2_02_fz_-10.txt" using 2:1 wi li lw 2 title "F_z * 0.90", "step2_02_fz_-15.txt" using 2:1 wi li lw 2 title "F_z * 0.85", 
