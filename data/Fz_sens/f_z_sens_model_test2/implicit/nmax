set terminal png size 1600, 1200
set output "n_max_sensitivity_model_impl_f_z.png"
set format x "%g"
set title "N_{max} sensitivity, model solution, implicit scheme"
set xlabel "n, cm^{-3}"
set ylabel "phi, deg"
unset logscale x
