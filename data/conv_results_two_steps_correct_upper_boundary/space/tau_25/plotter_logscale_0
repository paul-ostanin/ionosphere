set terminal png size 1600, 1200
set output "stationary_logscale_0.png"
set format x "%g"
set title "Stationary solutions (phi =0), tau = 25 s"
set xlabel "n, cm^{-3}"
set ylabel "z, km"
set logscale x
plot "step2_00_nz_80_nphi_180_tau_25.txt" using 2:1 wi li lw 2 title "Nphi = 180, Nz = 80", "step2_00_nz_200_nphi_360_tau_25.txt" using 2:1 wi li lw 2 title "Nphi = 360, Nz = 200"
