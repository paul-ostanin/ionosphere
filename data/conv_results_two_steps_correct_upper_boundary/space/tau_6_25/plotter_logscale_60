set terminal png size 1600, 1200
set output "stationary_logscale_60.png"
set format x "%g"
set title "Stationary solutions (phi = 60), tau = 6.25 s"
set xlabel "n, cm^{-3}"
set ylabel "z, km"
set logscale x
plot "step2_60_nz_80_nphi_180_tau_6.txt" using 2:1 wi li lw 2 title "Nphi = 180, Nz = 80", "step2_60_nz_200_nphi_360_tau_6.txt" using 2:1 wi li lw 2 title "Nphi = 360, Nz = 200", "step2_60_nz_400_nphi_720_tau_6.txt" using 2:1 wi li lw 2 title "Nphi = 720, Nz = 400"
