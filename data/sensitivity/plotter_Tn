set terminal png size 1024, 768
set output "Tn-sensitivity.Tnng"
set format x "%g"
set title "Tn-sensitivity"
set xlabel "n, cm^{-3}"
set ylabel "z, km"
unset logscale x
plot "Tn_08.txt" using 2:1 wi li lw 2 title "0.8*Tn", "Tn_09.txt" using 2:1 wi li lw 2 title "0.9*Tn", "Tn_10.txt" using 2:1 wi li lw 2 title "1.0*Tn", "Tn_11.txt" using 2:1 wi li lw 2 title "1.1*Tn", "Tn_12.txt" using 2:1 wi li lw 2 title "1.2*Tn"
