set terminal png size 1024, 768
set output "stationary_-30.png"
set format x "%g"
set title "Convergence (phi =-30)"
set xlabel "n, cm^{-3}"
set ylabel "z, km"
unset logscale x
plot "1d.txt" using 2:1 wi li lw 2 title "1d-scheme", "stationary_2d_-30.txt" using 2:1 wi li lw 2 title "scheme with d/dphi",  "stationary_z-projection_-30.txt" using 2:1 wi li lw 2 title "1d scheme with z-projection"
