set terminal png size 1024, 768
set output "nO-sensitivity.png"
set format x "%g"
set title "Convergence (phi =-10)"
set xlabel "n, cm^{-3}"
set ylabel "z, km"
unset logscale x
plot "1d.txt" using 2:1 wi li lw 2 title "1d-scheme", "phi_-10.txt" using 2:1 wi li lw 2 title "scheme with d/dphi", "sI_-10.txt" using 2:1 wi li lw 2 title "scheme with sin I"
